{
    "parser": "@babel/eslint-parser",
    "extends": ["eslint:recommended"],
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-shadow": 0,
      "no-param-reassign": 0,
      "prefer-object-spread": 0,
      "prefer-destructuring": 0,
      "import/named": 0,
      "no-unused-vars": "error"
    },
    "parserOptions": {
      "sourceType": "module"
    }
  }